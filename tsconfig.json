{
    "compilerOptions": {
        "target": "ES2020",
        "lib": [
            "ES2020"
        ],
        "module": "commonjs",
        "jsx": "react",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "moduleResolution": "node",
        "isolatedModules": true,
        "noEmit": true
    },
    "include": [
        "src/**/*"
    ]
}